				"method": "POST",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": "\r\n{\r\n   \"name\": \"{{name_dummy}}\",\r\n   \"salary\": {{salary_dummy}},\r\n    \"age\": {{age_dymmy}},\r\n    \"id\": {{id_r}}\r\n    }\r\n",
					"options": {
