	"item": [
		{
			"name": "Create",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"pm.test(\"Response time is less than 5000ms\", function () {\r",
							"    pm.expect(pm.response.responseTime).to.be.below(5000);\r",
							"});\r",
							"var jsonData = pm.response.json();\r",
							"\r",
							"pm.test(\"Response salary\", function () {\r",
							"    pm.expect(jsonData.data.salary).to.eql(pm.environment.get(\"salary_dummy\"));\r",
							"});\r",
							"pm.test(\"Response age\", function () {\r",
							"    pm.expect(jsonData.data.age).to.eql(pm.environment.get(\"age_dymmy\"));\r",
							"});"
						],
						"type": "text/javascript"
					}
				},
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"pm.environment.set(\"salary_dummy\", _.random(500, 5000));\r",
							"pm.environment.set(\"age_dymmy\", _.random(18, 100));\r",
							"\r",
							"pm.variables.replaceIn('{{$randomFirstName}}')\r",
							"pm.environment.set(\"name_dummy\", pm.variables.replaceIn('{{$randomFirstName}}'))\r",
							"pm.environment.set(\"id_r\", pm.variables.replaceIn('{{$randomBankAccount}}'))\r",
							"\r",
							""
						],
						"type": "text/javascript"
					}
				}
			],
